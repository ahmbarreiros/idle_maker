#include <windows.h>

int WINAPI WinMain(
		   HINSTANCE Instance,
		   HINSTANCE PrevInstance,
		   LPSTR     CmdLine,
		   int       ShowCmd
		   ) {
  HWND WindowHandle;
  WindowHandle = CreateWindowExA(
				 0,
				 LPCSTR    lpClassName,
				 "IdleMaker",
				 0,
				 CW_USEDEFAULT,
				 CW_USEDEFAULT,
				 CW_USEDEFAULT,
				 CW_USEDEFAULT,
				 NULL,
				 NULL,
				 Instance,
				 0
				 );

  return 0;
}
